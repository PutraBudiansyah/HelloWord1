<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/startFragment2">
    <fragment
        android:id="@+id/startFragment2"
        android:name="com.d3if4201.h_2ass.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_startFragment2_to_persegiFragment2"
            app:destination="@id/persegiFragment2" />
        <action
            android:id="@+id/action_startFragment2_to_quisFragment"
            app:destination="@id/quisFragment" />
        <action
            android:id="@+id/action_startFragment2_to_iceFragment"
            app:destination="@id/iceFragment" />
    </fragment>
    <fragment
        android:id="@+id/persegiFragment2"
        android:name="com.d3if4201.h_2ass.PersegiFragment"
        android:label="fragment_persegi"
        tools:layout="@layout/fragment_persegi" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.d3if4201.h_2ass.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/quisFragment"
        android:name="com.d3if4201.h_2ass.QuisFragment"
        android:label="fragment_quis"
        tools:layout="@layout/fragment_quis" >
        <action
            android:id="@+id/action_quisFragment_to_loseFragment"
            app:destination="@id/loseFragment" />
        <action
            android:id="@+id/action_quisFragment_to_winFragment4"
            app:destination="@id/winFragment4" />
    </fragment>
    <fragment
        android:id="@+id/loseFragment"
        android:name="com.d3if4201.h_2ass.LoseFragment"
        android:label="fragment_lose"
        tools:layout="@layout/fragment_lose" >
        <action
            android:id="@+id/action_loseFragment_to_quisFragment"
            app:destination="@id/quisFragment" />
    </fragment>
    <fragment
        android:id="@+id/winFragment4"
        android:name="com.d3if4201.h_2ass.WinFragment"
        android:label="fragment_win"
        tools:layout="@layout/fragment_win" >
        <action
            android:id="@+id/action_winFragment4_to_quisFragment"
            app:destination="@id/quisFragment" />
    </fragment>
    <fragment
        android:id="@+id/iceFragment"
        android:name="com.d3if4201.h_2ass.IceFragment"
        android:label="fragment_ice"
        tools:layout="@layout/fragment_ice" />
</navigation>